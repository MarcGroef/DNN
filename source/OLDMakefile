debug:

	gcc -Wall -std=gnu99 src/*.c src/DNN/src/*.c src/MNIST/src/*.c -o build/debug/dnn -lm -g 

val:
	gcc -Wall -std=gnu99 src/*.c src/DNN/src/*.c src/MNIST/src/*.c -o build/debug/dnn -lm -g 
	valgrind -v --track-origins=yes build/debug/dnn devel/MNIST_dataset/t10k-images.idx3-ubyte

release:

	gcc -Wall -std=gnu99 src/*.c src/DNN/src/*.c src/MNIST/src/*.c -o build/release/dnn -lm 

clean:
	rm -r -f build/release/*
	rm -r -f build/debug/*

	
run_debug:
	./build/debug

